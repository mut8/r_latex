      plot(ord, choices=c(1,2), type="n", tck=.01, 
      xlab=paste("PCA1", formatC(xvar[1]*100, digits=3), "% variance"),
      ylab=paste("PCA2", formatC(xvar[2]*100, digits=3), "% variance"))

      scores.sites<-(scores(ord, display="sites", choices=1:2))
