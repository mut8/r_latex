LCI_abs<-timeseries((orig_cTIC$L*(1-100*samples$massloss/c_lit)-h0.L) 
/ (orig_cTIC$L*(1-100*samples$massloss/c_lit) + orig_cTIC$C*(1-100*samples$massloss/c_lit) -h0.L - h0.C), days, type, pch=pch, col=colscale, endsig=T, legend="bottomleft")
abline(h=0)
