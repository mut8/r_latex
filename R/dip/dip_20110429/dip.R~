pr<-paste("R =", x$estimate, "p =", x$p.value)
print(pr)
mod<-processes[h3,h]~controls[h3,j]*controls[h3,i]
x<-summary(lm(mod))
print("lm")
print(x$coefficients)
x<-anova(lm(mod))
print("aov")
print(x)

print("harvest 2+3")
x<-cor.test(controls[,j], controls[,i])
pr<-paste("R =", x$estimate, "p =", x$p.value)
print(pr)
mod<-processes[,h]~controls[,j]*controls[,i]
x<-summary(lm(mod))
print("lm")
print(x$coefficients)
x<-anova(lm(mod))
print("aov")
print(x)
}
